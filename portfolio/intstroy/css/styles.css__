.wait {
	position: absolute;
	z-index:100;
	top: 0px;
	left: 0px;
	height:100%;
	text-indent:20px;
	width:100%;
}

.wait {
	background:#E2E2E2 url(../images/load_animate2.gif) no-repeat 50% 50%;
	filter:alpha(opacity=75);
	-moz-opacity: 0.75;
	opacity: 0.75;
}

* html .wait { /* ie6 hack */
	position: absolute;
	height: expression(document.this.parent.scrollHeight > document.this.parent.offsetHeight ? document.this.parent.scrollHeight : document.this.parent.offsetHeight + 'px');
}
