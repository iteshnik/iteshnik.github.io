html {
	background: #fffcec url(../images/bg-html.jpg) no-repeat 50% 0;
}

body {
	margin: 0;
	font: 13px / 16px Tahoma, Arial, sans-serif;
	background: url(../images/bg-body.jpg) repeat-x 0 100%;
	color: #484848;
	overflow-x:hidden;
	height:1%;
}

img {
	border: 0;
}

form, fieldset {
	margin: 0;
	padding: 0;
	border: none;
}

input, textarea, select {
	font-size: 100%;
	vertical-align: baseline;
}

a {
	color: #90150e;
}

a:hover {
	text-decoration: none;
}

.header .box:after, .header .heading:after, .main .m2:after, .main-heading .c:after, .product .c:after, .login-popup .c:after, .header .holder:after, .popup01 .c:after, .product-list:after, .main:after, .popup02 .c:after, .contacts ul.list:after, .contacts ul.list li:after {
	display: block;
	clear: both;
	content: " ";
}
#mian{

}
#tovimg {
	position: absolute;
	z-index: 100;
}

#tovimg img {
}

/*page*/
#page {
	margin: 0 auto;
	width: 1026px;
	/*overflow: hidden;*/
}

/*header*/
.header {
	height: 1%;
	color: #404040;
	position: relative;
	z-index: 10;
}

.header .holder {
	height: 1%;
	padding: 46px 27px 0 24px;
}

.logo {
	margin: 11px 0 0;
	float: left;
	overflow: hidden;
	text-indent: -9999px;
	width: 428px;
	height: 66px;
}

.logo a {
	display: block;
	height: 100%;
}

.header .frame {
	float: right;
	width: 475px;
}

.contacts {
	width: 450px;
	padding: 0 0 15px;
}

.header .heading {
	height: 1%;
	font: italic 24px / 26px "Times New Roman", Times, serif;
	padding: 0 0 10px;
	position: relative;
}

.header .heading a:hover {
	background: none;
}

.contacts ul.list {
	margin: 0;
	padding: 0;
	list-style: none;
	height: 1%;
	position: relative;
	z-index: 10;
	font: 12px / 16px Tahoma, Arial, sans-serif;
}

.contacts ul.list li {
	height: 1%;
	vertical-align: top;
	position: relative;
	z-index: 1;
}

.contacts ul.list li.show {
	z-index: 0;
}

.contact-box {
	width: 282px;
	float: left;
}

.contacts ul.list .icq {
	float: left;
	width: 101px;
	background: url(../images/icq.png) no-repeat;
	padding: 0 0 0 23px;
	margin: 2px 0 0;
	font: 12px / 16px Arial, Helvetica, sans-serif;
}

.contacts ul.list .numb {
	font: 18px / 20px "Times New Roman", Times, serif;
	padding-right: 20px;
	float: right;
}

.contacts ul.list a {
	color: #404040;
	background: url(../images/bg-dotted01.gif) repeat-x 0 100%;
	text-decoration: none;
	padding-bottom: 1px;
}

.contacts ul.list a:hover {
	background: none;
}

.popup02 {
	width: 309px;
	position: absolute;
	top: -15px;
	left: 105px;
	display: none;
	z-index: 10;
}

.contacts ul.list .show .popup02 {
	display: block;
}

.popup02 .t, .popup02 .b {
	width: 100%;
	height: 32px;
	overflow: hidden;
	background: url(../images/bg-popup02-t.png) no-repeat;
}

.popup02 .b {
	height: 18px;
	background: url(../images/bg-popup02-b.png) no-repeat;
}

.popup02 .c {
	height: 1%;
	padding: 0 10px 0 27px;
	background: url(../images/bg-popup02-c.png) repeat-y;
}

.popup02-holder {
	width: 100%;
	position: relative;
	margin: -17px 0 -2px;
	float: left;
}

.contacts ul.list .popup02 .contact-box {
	width: 262px;
}

.contacts ul.list .popup02 .numb {
	padding-right: 0;
}

.popup02 .row {
	height: 1%;
	overflow: hidden;
}

.popup02 .add-row {
	height: 1%;
	overflow: hidden;
	padding: 10px 11px 0 0;
}

.popup02 .add-row img {
	float: left;
}

.popup02 table, .popup02 table td {
	margin: 0;
	padding: 0;
	border-collapse: collapse;
	vertical-align: top;
	font-family: "Times New Roman", Times, serif;
	font-size: 10pt;
}

.popup02 table td.c_photo {
	padding: 5px;
	width: 100px;
}

.popup02 table td.c_data {
	padding: 5px 0 5px 5px;
}

.popup02 table td.c_data span {
	display: block;
	height: 20px;
	padding: 2px 0 2px 25px;
}

.popup02 table td.c_data span img {
}

.popup02 table td.c_data span.name {
	font-size: 14pt;
	line-height: 14pt;
	height: auto;
	padding: 0;
}

.popup02 table td.c_data span.whois {
	font-size: 12pt;
	line-height: 12pt;
	height: auto;
	padding: 0;
	font-style: italic;
	color: #666;
}

.contacts ul.list a.close, .contacts ul.list a.close:hover {
	float: right;
	width: 7px;
	height: 7px;
	text-indent: -9999px;
	overflow: hidden;
	margin: 5px 0 0;
	background: url(../images/ico03.gif) no-repeat;
}

.header .box {
	height: 1%;
}

.private {
	width: 248px;
	float: left;
	padding: 0 10px 0 0;
	position: relative;
	z-index: 3;
}

.private .heading {
	text-decoration: none;
}

.header .private .heading a {
	background: url(../images/bg-dotted02.gif) repeat-x 0 100%;
	text-decoration: none;
	float: left;
}

.header .private .heading a:hover {
	background: none;
}

.private p {
	margin: 0;
	font-size: 12px;
}

.private p a {
	color: #404040;
}

.login-popup {
	width: 221px;
	position: absolute;
	top: -10px;
	left: 62px;
	display: none;
}

.show .login-popup {
	display: block;
}

.heading .login-popup {
	width: 221px;
	position: absolute;
	top: -7px;
	left: 136px;
}

.login-popup .t, .login-popup .b {
	width: 100%;
	overflow: hidden;
	height: 33px;
	background: url(../images/bg-login-t.png) no-repeat;
}

.login-popup .b {
	height: 18px;
	background: url(../images/bg-login-b.png) no-repeat;
	margin-left: 8px;
}

.login-popup .c {
	height: 1%;
	background: url(../images/bg-login-c.png) repeat-y;
	margin-left: 8px;
}

.contacts ul.list li .login-popup .t, .contacts ul.list li .login-popup .b {
	width: 100%;
	overflow: hidden;
	height: 33px;
	background: url(../images/bg-login-t2.png) no-repeat;
	margin: 0;
}

.contacts ul.list li .login-popup .b {
	height: 18px;
	background: url(../images/bg-login-b2.png) no-repeat;
	1margin-left: 8px;
}

.contacts ul.list li .login-popup .c {
	height: 1%;
	background: url(../images/bg-login-c2.png) repeat-y;
	margin-left: 0px;
}

.login-popup-holder {
	float: left;
	width: 180px;
	padding: 0 17px 0 17px;
	position: relative;
	margin: -18px 0 -2px;
}

.login-popup .row {
	height: 1%;
	overflow: hidden;
	padding: 0 0 9px;
}

.input-text {
	display: block;
	height: 21px;
	overflow: hidden;
	background: url(../images/bg-input-text01.gif) no-repeat;
}

.input-text input.text {
	font: 11px Tahoma, Arial, sans-serif;
	border: 0;
	background: none;
	width: 224px;
	float: left;
	margin: 0;
	padding: 4px 7px;
	color: #808080;
}

.login-popup .btn-box {
	height: 1%;
	overflow: hidden;
}

.private .heading .login-popup .enter, .private .heading .login-popup .enter:hover {
	font: 11px / 14px Tahoma, Arial, sans-serif;
	background: url(../images/ico02.gif) no-repeat 0 5px;
	padding: 0 0 0 15px;
	text-decoration: underline;
}

.private .heading .login-popup .enter:hover {
	text-decoration: none;
}

.private .heading .login-popup .close, .private .heading .login-popup .close:hover {
	font: 11px / 14px Tahoma, Arial, sans-serif;
	float: right;
	text-indent: -9999px;
	width: 9px;
	height: 9px;
	margin: 3px 0 0;
	background: url(../images/ico03.gif) no-repeat 0 2px;
}

.basket {
	float: left;
	width: 217px;
	position: relative;
}

.basket .heading {
	background: url(../images/ico01.gif) no-repeat 100% 0;
	position: relative;
	margin: -4px 0 0;
