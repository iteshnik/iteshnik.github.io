}

.basket .heading2 {
	font: italic 24px / 26px "Times New Roman", Times, serif;
	background: url(../images/ico01.png) no-repeat 100% 0;
	padding: 4px 0 10px;
	position: absolute;
	top: -4px;
	color: #90150E;
	width: 100%;
	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=0); /* IE 5.5+*/
	-moz-opacity: 0; /* Mozilla 1.6 � ���� */
