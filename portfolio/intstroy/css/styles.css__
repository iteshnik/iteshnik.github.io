ul.tabset {
	margin:-18px 0 -33px -14px;
	padding:0 0 0 13px;
	list-style:none;
	height:1%;
	position:relative;
	overflow:hidden;
}
ul.tabset li {
	float:left;
	margin:0 -35px 0 0;
	padding:6px 0 20px;
	position:relative;
	font:12px/40px Arial, Helvetica, sans-serif;
}
ul.tabset #first {padding-bottom:0;}
ul.tabset a {
	position:relative;
	cursor:pointer;
}
ul.tabset a,
ul.tabset .l,
ul.tabset .c,
ul.tabset .r {float:left;}
ul.tabset .l,
ul.tabset .r {
	width:31px;
	height:55px;
	text-indent:-9999px;
	overflow:hidden;
	background:url(../images/bg-tab-l.png) no-repeat;
}
ul.tabset #first .l {height:75px;}
ul.tabset .r {background:url(../images/bg-tab-r.png) no-repeat;}
ul.tabset .c {
	background:url(../images/bg-tab-c.png) repeat-x;
	height:40px;
	padding:15px 0 0;
}
ul.tabset a:hover .l {background:url(../images/bg-tab-l02.png) no-repeat;}
ul.tabset a:hover .r {background:url(../images/bg-tab-r02.png) no-repeat;}
ul.tabset a:hover .c {background:url(../images/bg-tab-c02.png) repeat-x;}
ul.tabset li.active {padding:0; z-index:100;}
ul.tabset li.active .l {
	background:url(../images/bg-tab-l03.png) no-repeat;
	height:61px;
}
ul.tabset li#first.active .l {height:81px;}
ul.tabset li.active .r,
ul.tabset li.active a:hover .r {
	background:url(../images/bg-tab-r03.png) no-repeat;
	height:61px;
}
ul.tabset li.active .c {
	background:url(../images/bg-tab-c03.png) repeat-x;
	padding:21px 5px 0;
}
ul.tabset li.active a {
	color:#404040;
	text-decoration:none;
}


