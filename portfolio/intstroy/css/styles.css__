/*** rur ***/
@font-face {
	font-family:"Ruble Arial";
	src:url("../fonts/ruble_arial.ttf");
}
span.rur {
    font: normal 10pt / 1.4 "Ruble Arial" !important;
}

span.rur span {
    display: none;
}

span.rur span:before {
    content: '\2013';
}

